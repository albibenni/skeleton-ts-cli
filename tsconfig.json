{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2016",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "sourceMap": true
  }
}
